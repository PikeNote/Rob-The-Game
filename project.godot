; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Button",
"class": "LetterInventory",
"language": "GDScript",
"path": "res://Presets/Letter Stuff/LetterInv.gd"
} ]
_global_script_class_icons={
"LetterInventory": ""
}

[application]

config/name="Rob"
run/main_scene="res://Screens/MainScene.tscn"
boot_splash/image="res://Assets/Splash_Screen.png"
config/icon="res://Assets/Game_Logo.PNG"

[autoload]

Words="*res://Global Stuff/WordManager.gd"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
GameReferences="*res://Global Stuff/GameReferences.gd"
SilentWolfGlobal="*res://Global Stuff/SilentWolf.gd"
MultiplayerWebsocket="*res://Global Stuff/MultiplayerWebsocket.gd"
UserManager="*res://Global Stuff/UserManager.gd"
GameParameters="*res://Global Stuff/GameParameters.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/animated_grid_container_plugin/plugin.cfg", "res://addons/silent_wolf/plugin.cfg" )

[gdnative]

singletons=[  ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
